<%= form_for :company do |f| %>
   <p>
    <%= f.label :Name %><br>
    <%= f.text_area :Name %>
  </p>
  <p>
    <%= f.label :Address %><br>
    <%= f.text_area :Address %>
  </p>
  <p>
    <%= f.label :NIF %><br>
    <%= f.text_field :NIF %>
  </p>
  <p>
    <%= f.label :CreatedBy %><br>
    <%= f.text_field :CreatedBy %>
  </p>
  <p">
    <%= f.label :UpdatedBy %><br>
    <%= f.text_field :UpdatedBy %>
  </p>
<% end %>